  <connectionStrings>
    <add name="TempHireDbContext" connectionString="Data Source=|DataDirectory|TempHire.sdf" providerName="System.Data.SqlServerCe.4.0" />
    <add name="Security" connectionString="Data Source=|DataDirectory|TempHire.Security.sdf" providerName="System.Data.SqlServerCe.4.0" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlCeConnectionFactory, EntityFramework">
